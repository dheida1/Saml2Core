<?xml version="1.0"?>
<package >
  <metadata>
    <id>Saml2Core</id>
    <version>3.0</version>
    <title>Saml2 for .Net Core</title>
    <authors>Dina Heidar</authors>
    <owners>Dina Heidar</owners>
    <language>en-US</language>
    <projectUrl>https://git.la.gov/OTS/SAML2</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://mit-license.org/</licenseUrl>
    <description>ASP.NET Core middleware that enables an application to support the SAML2 SSO authentication workflow. This middleware is opensource (free to use) and is not dependent on any .NET Framework libraries. It has been tested with ADFS and IdentityServer4 as well.</description>
    <releaseNotes>Package updates to work with .NetCore 3.1.</releaseNotes>
    <copyright>Copyright 2019</copyright>
    <tags>saml .net core adfs authentication</tags>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Cryptography.X509Certificates.Extension" version="1.0.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.AspNetCore.Authentication" version="2.2.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.AspNetCore.Authentication.Cookies" version="2.2.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.IdentityModel.Protocols" version="5.6.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.IdentityModel.Tokens.Saml" version="5.6.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.IdentityModel.Xml" version="5.6.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="readme.txt" target="" />
    <file src="bin\Debug\netstandard2.0\*.dll" target="lib\netstandard2.0" />
  </files>
</package>